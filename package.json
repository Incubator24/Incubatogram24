{
  "name": "app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "set NODE_ENV=development&& nest start --watch",
    "start:dev auth": "set NODE_ENV=development&& nest start auth --watch",
    "start:test": "set NODE_ENV=test&&nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "set NODE_ENV=production&&node dist/apps/app/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/app/test/jest-e2e.json",
    "build:app": "nest build app",
    "start:app": "node dist/apps/app/main",
    "start:dev:app": "nest start app --watch",
    "build:auth": "nest build auth",
    "start:auth": "node dist/apps/auth/main",
    "start:dev:auth": "nest start auth --watch",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:seed": "ts-node ./prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.445.0",
    "@nestjs/common": "10.0.0",
    "@nestjs/config": "3.2.2",
    "@nestjs/core": "10.0.0",
    "@nestjs/cqrs": "10.2.7",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/microservices": "10.3.8",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.0.0",
    "@nestjs/swagger": "7.3.1",
    "@nestjs/throttler": "5.1.2",
    "@prisma/client": "5.19.1",
    "@types/nodemailer": "6.4.15",
    "@types/passport-google-oauth20": "2.0.16",
    "and": "0.0.3",
    "axios": "1.3.1",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cookie-parser": "1.4.6",
    "date-fns": "3.6.0",
    "dotenv": "16.4.5",
    "dotenv-flow": "4.1.0",
    "eslint-plugin-react": "7.34.1",
    "from": "0.1.7",
    "import": "0.0.6",
    "jsonwebtoken": "9.0.2",
    "nodemailer": "6.9.13",
    "passport": "0.7.0",
    "passport-github2": "0.1.12",
    "passport-google-oauth20": "2.0.0",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "pg": "8.11.5",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "stripe": "^17.3.1",
    "swagger-ui-express": "5.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.0.0",
    "@nestjs/schematics": "10.0.0",
    "@nestjs/testing": "10.0.0",
    "@types/bcryptjs": "2.4.6",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.2",
    "@types/multer": "1.4.11",
    "@types/node": "20.3.1",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.5.0",
    "prettier": "3.2.5",
    "prisma": "5.19.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}